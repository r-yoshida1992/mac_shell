#!/bin/zsh
# Check if the "/Applications/Visual Studio Code - Insiders.app" directory exists
if [ -d "/Applications/Visual Studio Code - Insiders.app" ]; then
    # If it exists, open "Visual Studio Code - Insiders" with the current directory
    open -a "Visual Studio Code - Insiders" .
else
    # If it doesn't exist, open "Visual Studio Code" with the current directory
    open -a "Visual Studio Code" .
fi
